<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.enigma.enigmat.model.Product" table="mst_product">
        <id name="productId" type="java.lang.Integer" column="id"/>
        <property name="productName" type="java.lang.String" column="name"/>
        <property name="productPrice" type="java.lang.Integer" column="product_price"/>
        <property name="stock" type="java.lang.Integer" column="stock"/>
    </class>
</hibernate-mapping>
